    <diagramLink
        href="vespucci.gmfgraph#InAndOut"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/target"/>
    <creationConstraints>
      <sourceEnd
          body="(self &lt;> oppositeEnd) and (not self.oclIsTypeOf(Empty))"/>
      <targetEnd
          body="not self.oclIsTypeOf(Empty)"/>
    </creationConstraints>
  </links>
  <links>
    <domainMetaElement
        href="vespucci.ecore#//NotAllowed"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="vespucci.gmfgraph#NotAllowedName"/>
      <features
          href="vespucci.ecore#//Connection/name"/>
      <editableFeatures
          href="vespucci.ecore#//Connection/name"/>
    </labelMappings>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="vespucci.gmftool#//@palette/@tools.1/@tools.4"/>
    <diagramLink
        href="vespucci.gmfgraph#NotAllowed"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/target"/>
    <creationConstraints>
      <sourceEnd
          body="(self &lt;> oppositeEnd) and (not self.oclIsTypeOf(Empty))"/>
      <targetEnd
          body="not self.oclIsTypeOf(Empty)"/>
    </creationConstraints>
  </links>
  <links>
    <domainMetaElement
        href="vespucci.ecore#//Expected"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="vespucci.gmfgraph#ExpectedName"/>
      <features
          href="vespucci.ecore#//Connection/name"/>
      <editableFeatures
          href="vespucci.ecore#//Connection/name"/>
    </labelMappings>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="vespucci.gmftool#//@palette/@tools.1/@tools.3"/>
    <diagramLink
        href="vespucci.gmfgraph#Expected"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/target"/>
    <creationConstraints>
      <sourceEnd
          body="(self &lt;> oppositeEnd) and (not self.oclIsTypeOf(Empty))"/>
      <targetEnd
          body="not self.oclIsTypeOf(Empty)"/>
    </creationConstraints>
  </links>
  <links>
    <domainMetaElement
        href="vespucci.ecore#//GlobalIncoming"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="vespucci.gmfgraph#GlobalIncomingName"/>
      <features
          href="vespucci.ecore#//Connection/name"/>
      <editableFeatures
          href="vespucci.ecore#//Connection/name"/>
    </labelMappings>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="vespucci.gmftool#//@palette/@tools.1/@tools.5"/>
    <diagramLink
        href="vespucci.gmfgraph#GlobalIncoming"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/target"/>
    <creationConstraints>
      <sourceEnd
          body="(self &lt;> oppositeEnd) "/>
      <targetEnd
          body="not self.oclIsTypeOf(Empty)"/>
    </creationConstraints>
  </links>
  <links>
    <domainMetaElement
        href="vespucci.ecore#//GlobalOutgoing"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="vespucci.gmfgraph#GlobalOutgoingName"/>
      <features
          href="vespucci.ecore#//Connection/name"/>
      <editableFeatures
          href="vespucci.ecore#//Connection/name"/>
    </labelMappings>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"

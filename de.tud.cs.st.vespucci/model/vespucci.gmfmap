<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/shapes"/>
    <ownedChild>
      <domainMetaElement
          href="vespucci.ecore#//Ensemble"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="vespucci.gmfgraph#EnsembleName"/>
        <features
            href="vespucci.ecore#//Shape/name"/>
      </labelMappings>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping"
          viewPattern=""
          editorPattern=""
          editPattern="">
        <diagramLabel
            href="vespucci.gmfgraph#EnsembleDescription"/>
        <features
            href="vespucci.ecore#//Shape/description"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="vespucci.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="vespucci.gmfgraph#Ensemble"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0"
          referencedChild="//@nodes.0/@ownedChild">
        <containmentFeature
            href="vespucci.ecore#//Ensemble/shapes"/>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0"
          referencedChild="//@nodes.1/@ownedChild">
        <containmentFeature
            href="vespucci.ecore#//Ensemble/shapes"/>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0 //@nodes.0/@ownedChild/@children.1">
        <compartment
            href="vespucci.gmfgraph#EnsembleCompartment"/>
      </compartments>
      <compartments>
        <compartment
            href="vespucci.gmfgraph#EnsembleDescriptionCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/shapes"/>
    <ownedChild>
      <domainMetaElement
          href="vespucci.ecore#//Empty"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="vespucci.gmfgraph#EmptyName"/>
        <features
            href="vespucci.ecore#//Shape/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="vespucci.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="vespucci.gmfgraph#Empty"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="vespucci.ecore#//Incoming"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="vespucci.gmfgraph#IncomingName"/>
      <features
          href="vespucci.ecore#//Connection/name"/>
      <editableFeatures
          href="vespucci.ecore#//Connection/name"/>
    </labelMappings>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="vespucci.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="vespucci.gmfgraph#Incoming"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/target"/>
    <creationConstraints>
      <sourceEnd
          body="self &lt;> oppositeEnd"/>
      <targetEnd
          body="not self.oclIsTypeOf(Empty)"/>
    </creationConstraints>
  </links>
  <links>
    <domainMetaElement
        href="vespucci.ecore#//Outgoing"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping"
        editorPattern=""
        editPattern="">
      <diagramLabel
          href="vespucci.gmfgraph#OutgoingName"/>
      <features
          href="vespucci.ecore#//Connection/name"/>
      <editableFeatures
          href="vespucci.ecore#//Connection/name"/>
    </labelMappings>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="vespucci.gmftool#//@palette/@tools.1/@tools.1"/>
    <diagramLink
        href="vespucci.gmfgraph#Outgoing"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="vespucci.ecore#//Connection/target"/>
    <creationConstraints>
      <sourceEnd
          body="(self &lt;> oppositeEnd) and (not self.oclIsTypeOf(Empty))"/>
    </creationConstraints>
  </links>
  <links>
    <domainMetaElement

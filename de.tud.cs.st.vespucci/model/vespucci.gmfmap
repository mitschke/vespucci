    </creationConstraints>
  </links>
  <diagram>
    <diagramCanvas
        href="vespucci.gmfgraph#vespucci_model"/>
    <domainModel
        href="vespucci.ecore#/"/>
    <domainMetaElement
        href="vespucci.ecore#//ArchitectureModel"/>
    <palette
        href="vespucci.gmftool#//@palette"/>
  </diagram>
  <audits
      id="VespucciDiagramConstraintsID"
      name="Vespucci Diagram Constraints"
      description="All diagram related constraints for Vespucci Editor.">
    <childContainers
        id="EnsembleConstraintsID"
        name="All constraints about Ensemble.">
      <audits
          name="ERROR if Ensemble name not NULL"
          id="EnsembleNameNotNullID"
          message="Ensemble name must be set!">
        <rule
            body="name.size()>0 and not name.oclIsUndefined()"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="vespucci.ecore#//Ensemble"/>
        </target>
      </audits>
      <audits
          name="WARNING if Ensemble query not NULL"
          id="EnsembleQueryNotNullID"
          severity="WARNING"
          message="Ensemble query is not set! Default query is &quot;empty&quot;.">
        <rule
            body="query.size()>0 and not query.oclIsUndefined()"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="vespucci.ecore#//Ensemble"/>
        </target>
      </audits>
      <audits
          name="ERROR if non leaf Ensemble is not derived"
          description="non leaf Ensemble must be derived / empty"
          id="NonLeafEnsembleMustBeDerivedID"
          message="Queries of non leaf ensemble must be derived ">
        <rule
            body="NonLeafEnsembleMustBeDerived"
            language="java"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="vespucci.ecore#//Ensemble"/>
        </target>
      </audits>
      <audits
          name="WARNING if Ensemble query has invalid parenthesis count"
          description="Ensemble query invalid parenthesis count."
          id="EnsembleQueryValidParenthesisID"
          severity="WARNING"
          message="Ensemble query invalid parenthesis count.">
        <rule
            body="ValidParenthesisCount"
            language="java"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="vespucci.ecore#//Ensemble"/>
        </target>
      </audits>
      <audits
          name="WARNING if Ensemble query contains incomplete Prolog string"
          description="Ensemble query contains incomplete Prolog strings."
          id="EnsembleQueryCompletePrologStringsID"
          severity="WARNING"
          message="Missing ' in Ensemble query.">
        <rule
            body="CompletePrologString"
            language="java"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="vespucci.ecore#//Ensemble"/>
        </target>
      </audits>
      <audits
          name="WARNING if Ensemble query has invalid bracket count"
          description="Ensemble query has invalid bracket count."
          id="EnsembleQueryValidBracketID"
          severity="WARNING"
          message="Invalid brackets count in Ensemble query.">
        <rule
            body="ValidBracketCount"
            language="java"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="vespucci.ecore#//Ensemble"/>
        </target>
      </audits>
    </childContainers>
    <childContainers
        id="ConnectionsConstraintsID"
        name="All constraints about Connections.">
      <audits
          name="invalid constraint-dependency"
          description="Dependencies must be derived from a set of valid keywords."
          id="DependencyInvalidID"
          message="Dependency kind is invalid.">
        <rule
            body="ValidConstraintDependency"
            language="java"/>
        <target
            xsi:type="gmfmap:DomainAttributeTarget">
          <attribute
              href="vespucci.ecore#//Connection/name"/>
        </target>
      </audits>
    </childContainers>
  </audits>
</gmfmap:Mapping>

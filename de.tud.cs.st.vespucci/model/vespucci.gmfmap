    </creationConstraints>
  </links>
  <links>
    <domainMetaElement
        href="vespucci.ecore#//Violation"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="vespucci.gmfgraph#ViolationName"/>
      <features
          href="vespucci.ecore#//Connection/name"/>
      <editableFeatures
          href="vespucci.ecore#//Connection/name"/>
    </labelMappings>
    <containmentFeature
        href="vespucci.ecore#//ArchitectureModel/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"

   <extension point="org.eclipse.emf.validation.constraintProviders">
      <?gmfgen generated="true"?>
            <category id="VespucciDiagramConstraintsID" mandatory="false" name="Vespucci Diagram Constraints">
         <![CDATA[All diagram related constraints for Vespucci Editor.]]>
      </category>
      <category id="VespucciDiagramConstraintsID/EnsembleConstraintsID" mandatory="false" name="All constraints about Ensemble.">
         <![CDATA[]]>
      </category>
      <category id="VespucciDiagramConstraintsID/ConnectionsConstraintsID" mandatory="false" name="All constraints about Connections.">
         <![CDATA[]]>
      </category>
      <constraintProvider cache="true">
         <package namespaceUri="http://vespucci.editor/2011-06-01"/>
               <constraints categories="VespucciDiagramConstraintsID">
                  </constraints>
         <constraints categories="VespucciDiagramConstraintsID/EnsembleConstraintsID">
                  <constraint id="EnsembleNameNotNullID"
            lang="OCL"
            name="ERROR if Ensemble name not NULL"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[name.size()>0 and not name.oclIsUndefined()]]>
            <description><![CDATA[]]></description>
            <message><![CDATA[Ensemble name must be set!]]></message>
            <target class="vespucci_model.Ensemble"/>
         </constraint>
                  <constraint id="EnsembleNameUniqueID"
            lang="OCL"
            name="ERROR if Ensemble name UNIQUE"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[Ensemble.allInstances()->forAll(e| e <> self implies e.name <> self.name)]]>
            <description><![CDATA[]]></description>
            <message><![CDATA[Ensemble must have unique name!]]></message>
            <target class="vespucci_model.Ensemble"/>
         </constraint>
                  <constraint id="EnsembleQueryNotNullID"
            lang="OCL"
            name="WARNING if Ensemble query not NULL"
            mode="Batch"
            severity="WARNING" statusCode="200">
            <![CDATA[query.size()>0 and not query.oclIsUndefined()]]>
            <description><![CDATA[]]></description>
            <message><![CDATA[Ensemble query is not set! Default query is "empty".]]></message>
            <target class="vespucci_model.Ensemble"/>
         </constraint>
                  <constraint id="NonLeafEnsembleMustBeDerivedID"
            lang="Java" class="de.tud.cs.st.vespucci.vespucci_model.diagram.providers.VespucciValidationProvider$Adapter4"
            name="ERROR if non leaf Ensemble is not derived"
            mode="Batch"
            severity="ERROR" statusCode="200">
            
            <description><![CDATA[non leaf Ensemble must be derived / empty]]></description>
            <message><![CDATA[Queries of non leaf ensemble must be derived ]]></message>
            <target class="vespucci_model.Ensemble"/>
         </constraint>
                  </constraints>
         <constraints categories="VespucciDiagramConstraintsID/ConnectionsConstraintsID">
                  </constraints>
      </constraintProvider>
   </extension>

